    margin-right: 0 !important;
    margin-top: 0 !important;
}

h2.home_middle {
    float: left;
    font-family: "The Girl Next Door",cursive;
    font-size: 34px;
}

.home_middle.top_h2 {
    margin-top: 0;
}
.home_middle_blocks {
	background-color: #4cbb17 /* #00FF99*/;
	float: left;
	padding: 20px;
	width: 50%;
	font-size: 17px;
	min-height: 200px;
	line-height: 23px;
}

.circle {
    background-color: #faffff;
    border-radius: 80px;
    height: 100px;
    margin-left: -115px;
    margin-top: 30px;
    position: absolute;
    width: 100px;
}

.home_middle_blocks.two, .home_middle_blocks.four{
    padding-left: 60px;
}

.home_middle_blocks.one, .home_middle_blocks.three {
    border-right: 5px solid #faffff;
}


.home_middle_blocks.two > p,
.home_middle_blocks.four > p,
{
    padding-left: 20px;
}



.home_middle_blocks > a {
    background-color: #faffff;
    color: #333;
    float: left;
    font-size: 14px !important;
    font-weight: normal;
    margin-left: 35%;
    padding: 5px;
    text-align: center;
    width: 200px !important;
}

