.big_image_slide{
  width: 100%; min-height: 460px; background: url("/uploads/slides/slide1.jpg") repeat scroll 0% 0% transparent; float: left;
   padding-top: 100px;
	  text-align: center;
	  color: #fff;
}

.search_box_wide {
 
    background-image: url("/templates/default/images/search_box_wide_bg.png");
    margin-top: 350px;
    padding-bottom: 45px;
    padding-top: 45px;
    position: absolute;
    width: 100%;
	
	
}

