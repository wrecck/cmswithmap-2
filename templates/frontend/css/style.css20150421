}


.main_wrap{max-width: 500px; margin: 0 auto 0; background: #ffffff; border-radius: 3px; overflow: hidden; padding-bottom:20px;}
.main_wrap_pad{padding: 25px;}
.main_content_width{width:80%; margin:auto;}
.add_new_listing_left {
    float: left;
    width: 60%;
	padding-right: 20px;
}
.logo {
    color: #006600 !important;
    font-family: "Trebuchet MS";
    font-size: 38px !important;
    font-weight: bold;
    text-shadow: 3px 2px 4px #308014;
}

.logo > span {
    color: #333;
}
.logo:hover{
  color:#006600; text-decoration:none;
}

.add_new_listing_right {
    background-color: #f5f5f5;
